<body>
    <header class="header-container py-3">
        <div class="breadcrumbs col-12 py-2">
            <div class="row d-flex align-items-center justify-content-center justify-content-md-between text-center text-md-left">
                <div class="col-md-6 mb-2">
                    @if (Session["supervisor"] != null)
                    {
                        <a class="logo" href="supervisor"><img src="/images/logo.png" alt="C3i" width="80" height="auto" /></a>
                    }
                    else
                    {
                        <a class="logo" href="index"><img src="/images/logo.png" alt="C3i" width="80" height="auto" /></a>
                    }
                </div><!-- logo -->
                <div class="col-md-6 text-md-right mb-2">
                    <div class="dropdown d-inline-block">
                        <a href="javascript:void(0)" class="" data-toggle="dropdown">
                            <img src="/images/mark.png" alt="Mark Anthony" width="46" height="46" class="rounded-circle" /><span class="ml-2 welcome-message">@ViewData["userName"].ToString()</span>
                        </a>
                        <input type="button" value="logout" class="btn btn-secondary"
                               onclick="location.href='@Url.Action("logout", "C3_Fedex")'" />
                        <div class="dropdown-menu col-12">
                            <a class="dropdown-item" href="#">My Account</a>
                            <a class="dropdown-item" href="#">Logout</a>
                        </div>
                    </div><!-- drop down account-->
                </div>
            </div>
        </div>
    </header><!--.header-container-->
    <section class="content-container bglight-gray">
        <!--<div class="breadcrumbs col-12 py-2" style="background-color:'#1D5288'">
            <ul class="pl-0 mb-0" ><li >Home</li></ul>
        </div>//breadcrumbs-->

        <div class="container-fluid">
            @using (Html.BeginForm(FormMethod.Post))
            {
                <div class="table-responsive">
                    <table style="table-layout:fixed;width: 100%;" class="table table-sm line">
                        <thead>
                            <tr>
                                @*<th style="text-align: center;" id="select">Select Id</th>*@
                                <th style="text-align: center;" id="Case_ID">Case ID</th>
                                <th style="text-align: center;" id="Tracking">Tracking #</th>
                                <th style="text-align: center;" id="Title">Title</th>
                                <th style="text-align: center;" id="Alternate">Alternate Title</th>
                                <th style="text-align: center;" id="Commitment">Commitment Date </th>
                                <th style="text-align: center;" id="Commitment">Follow-Up Date and Time</th>
                                <th style="text-align: center;" id="Shipper">Shipper's Local Time</th>
                                <th style="text-align: center;" id="Attempts">Attempts Made</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in @Model.indexmodel.ToList())
                            {
                                <tr>
                                    @*<td> <input type="checkbox" name="departments" id="chck" value="@item.case_id" /></td>*@
                                    @if (Session["supervisor"] != null)
                                    {
                                        <td> @Html.ActionLink(@item.case_id, "supervisor", new { @item.case_id })</td>
                                    }
                                    else
                                    {
                                        <td> @Html.ActionLink(@item.case_id, "index", new { @item.case_id })</td>
                                    }
                                    @*<td>@item.case_id  </td>*@
                                    <td>@item.tracking  </td>
                                    <td>@item.title  </td>
                                    <td>@item.alternate_title  </td>
